{
    "name": "codebar-ag/laravel-zammad",
    "description": "Zammad integration with Laravel",
    "keywords": [
        "Codebar AG",
        "laravel",
        "laravel-zammad"
    ],
    "homepage": "https://github.com/codebar-ag/laravel-zammad",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruslan Steiger",
            "email": "ruslan.steiger@codebar.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "CodebarAg\\Zammad\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodebarAg\\Zammad\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodebarAg\\Zammad\\ZammadServiceProvider"
            ],
            "aliases": {
                "Zammad": "CodebarAg\\Zammad\\ZammadFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
