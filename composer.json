{
    "name": "codebar-ag/laravel-zammad",
    "description": "Zammad integration with Laravel",
    "keywords": [
        "zammad",
        "laravel",
        "codebar-ag",
        "laravel-zammad"
    ],
    "homepage": "https://github.com/codebar-ag/laravel-zammad",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Steiger",
            "email": "sebastian.fix@codebar.ch",
            "role": "Developer"
        },
        {
            "name": "Ruslan Steiger",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/contracts": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friendsofphp/php-cs-fixer": "3.0.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15|^7.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "CodebarAg\\Zammad\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodebarAg\\Zammad\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodebarAg\\Zammad\\ZammadServiceProvider"
            ],
            "aliases": {
                "Zammad": "CodebarAg\\Zammad\\Facades\\Zammad"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
